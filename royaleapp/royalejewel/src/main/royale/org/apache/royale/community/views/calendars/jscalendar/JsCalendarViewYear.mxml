<?xml version="1.0" encoding="utf-8"?>
<j:Card xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns:j="library://ns.apache.org/royale/jewel"
        xmlns:js="library://ns.apache.org/royale/basic"
        xmlns:html="library://ns.apache.org/royale/html"
        xmlns:jc="library://ns.apache.org/royale/community" 
	    itemsExpand="false">

    <fx:Script>
        <![CDATA[

            [Bindable]
            private var year:Number;

            private function onInit(event:Event):void{
                
            }
        ]]>
    </fx:Script>

    <j:beads>
		<js:ContainerDataBinding/>
	</j:beads>
    
    <j:CardHeader>
        <j:BarSection width="100%" gap="10">
            <html:H3 text="jsCalendar - Royale Custom Control (Control AnnualJSCalendar)" className="primary-darkest block-with-text "/>
        </j:BarSection>
    </j:CardHeader>
    <j:CardExpandedContent itemsHorizontalAlign="itemsCenter" itemsVerticalAlign="itemsCenter" className="primary-dark">
            <html:H3 text="Simple - auto Calendar"/>
            <j:Label multiline="true">
                <j:html><![CDATA[<p><b>AnnualJSCalendar</b></p>]]></j:html>
            </j:Label>
    </j:CardExpandedContent>

    <j:CardPrimaryContent>
       <jc:AnnualJSCalendar localId="vAnnu" percentWidth="100" percentHeight="100"
        year="{year}"/>

    </j:CardPrimaryContent>

</j:Card>